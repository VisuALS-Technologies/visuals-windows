<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:VisuALS_WPF_App">

    <SolidColorBrush x:Key="SweetCherry" Color="#FF6583"/>
    <SolidColorBrush x:Key="PinkLemonade" Color="#FF73EC"/>
    <SolidColorBrush x:Key="ElectricGrape" Color="#B350FF"/>
    <SolidColorBrush x:Key="BlueRaspberry" Color="#5164FF"/>
    <SolidColorBrush x:Key="BubbleBlue" Color="#51ADFF"/>
    <SolidColorBrush x:Key="GreenApple" Color="#61E39D"/>
    <SolidColorBrush x:Key="FreshLemon" Color="#F1E36C"/>
    <SolidColorBrush x:Key="VitaminC" Color="#FFB06E"/>
    <SolidColorBrush x:Key="SugarPlum" Color="#C679FF"/>

    <SolidColorBrush x:Key="BrilliantBlue" Color="#0056FF"/>

    <SolidColorBrush x:Key="White" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="Silver" Color="#CFCFCF"/>
    <SolidColorBrush x:Key="Iron" Color="#ADADAD"/>
    <SolidColorBrush x:Key="Lead" Color="#82829A"/>
    <SolidColorBrush x:Key="Pewter" Color="#585967"/>
    <SolidColorBrush x:Key="Obsidian" Color="#3F3F55"/>
    <SolidColorBrush x:Key="Shadow" Color="#3E3E48"/>
    <SolidColorBrush x:Key="Umbra" Color="#34343E"/>
    <SolidColorBrush x:Key="Black" Color="#000000"/>

    <SolidColorBrush x:Key="LightAccentColor" Color="#BFD6FF"/>
    <SolidColorBrush x:Key="AccentColor" Color="#0056FF"/>
    <SolidColorBrush x:Key="DarkAccentColor" Color="#001640"/>

    <LinearGradientBrush x:Key="Coastline" StartPoint="0, 0" EndPoint="1, 1">
        <GradientStop Color="#48e6ff" Offset="0.1"/>
        <GradientStop Color="#ffe8bf" Offset="0.8"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="DeepSea" Color="#24698a"/>

    <SolidColorBrush x:Key="DarkGlass" Color="#000000" Opacity="0.33"/>
    <SolidColorBrush x:Key="LightGlass" Color="#ffffff" Opacity="0.33"/>

</ResourceDictionary>